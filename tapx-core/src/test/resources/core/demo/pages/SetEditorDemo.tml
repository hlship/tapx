<t:layout xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:x="tapestry-library:tapx">


  <h2>Input Form</h2>

  <t:form>

    <x:seteditor set="values" label="Names" />

    <br clear="both"/>

    <input type="submit" value="Update"/>
  </t:form>


  <h2>Output</h2>

  <ul id="submitted-values">
    <li t:type="loop" source="values" value="value">
      ${value}
      </li>
  </ul>

</t:layout>